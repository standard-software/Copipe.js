{
  "private": false,
  "name": "@standard-software/copipe",
  "version": "0.7.4",
  "description": "simple code parts",
  "main": "./release/node/copipe",
  "scripts": {
    "tsc:node": "tsc -p ./tsconfig.node.json",
    "start:node:debug": "node ./scripts/start_node_debug.js",
    "start:node:release": "node ./scripts/start_node_release.js",
    "start:node": "npm run start:node:debug && npm run start:node:release",

    "tsc:web": "tsc -p ./tsconfig.web.json",
    "start:web:debug": "node ./scripts/start_web_debug.js",
    "start:web:release": "node ./scripts/start_web_release.js",
    "start:web": "npm run start:web:debug && npm run start:web:release",

    "tsc:wsh": "tsc -p ./tsconfig.wsh.json",
    "start:wsh:debug": "node ./scripts/start_wsh_debug.js",

    "start:all": "npm run start:node && npm run start:web",
    "test": ""
  },
  "author": "Satoshi Yamamoto",
  "devDependencies": {
    "@types/node": "^11.10.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "opener": "^1.5.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "license": "MIT"
}
